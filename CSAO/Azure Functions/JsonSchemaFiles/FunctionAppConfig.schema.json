{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "functionApps"
  ],
  "properties": {
    "functionApps": {
      "$id": "#/properties/functionApps",
      "type": "array",
      "title": "The Functionapps Schema",
      "items": {
        "$id": "#/properties/functionApps/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "id",
          "name",
          "functionKey",
          "resourceURI"
        ],
        "properties": {
          "id": {
            "$id": "#/properties/functionApps/items/properties/id",
            "type": "string",
            "title": "The Id Schema",
            "default": "",
            "examples": [
              "AppService"
            ],
            "pattern": "^(.*)$"
          },
          "name": {
            "$id": "#/properties/functionApps/items/properties/name",
            "type": "string",
            "title": "The Name Schema",
            "default": "",
            "examples": [
              "azusgrappservice"
            ],
            "pattern": "^(.*)$"
          },
          "functionKey": {
            "$id": "#/properties/functionApps/items/properties/functionKey",
            "type": "string",
            "title": "The Functionkey Schema",
            "default": "",
            "examples": [
              "VTuCXtUVjJmj1eX8JXXJ65khZcwtvTUmsWk3QHK49cfgS695tRNUTw=="
            ],
            "pattern": "^(.*)$"
          },
          "resourceURI": {
            "$id": "#/properties/functionApps/items/properties/resourceURI",
            "type": "string",
            "title": "The Resourceuri Schema",
            "default": "",
            "examples": [
              ""
            ],
            "pattern": "^(.*)$"
          }
        }
      }
    }
  }
}